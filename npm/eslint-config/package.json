{
  "name": "@maxpatiiuk/eslint-config",
  "version": "6.0.0",
  "private": false,
  "type": "module",
  "author": "Max Patiiuk",
  "description": "A 'best practices' ESLint config with support for TypeScript and Prettier",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxpatiiuk/dotfiles",
    "directory": "npm/eslint-config"
  },
  "homepage": "https://github.com/maxpatiiuk/dotfiles/tree/main/npm/eslintrc/#readme",
  "bugs": {
    "url": "https://github.com/maxpatiiuk/dotfiles/issues"
  },
  "scripts": {
    "build": "tsup",
    "prepublish": "npm run build",
    "publish": "npm publish --access public"
  },
  "main": "eslint.config.js",
  "module": "eslint.config.js",
  "exports": {
    ".": {
      "types": "./eslint.config.d.ts",
      "import": "./eslint.config.js",
      "require": "./eslint.config.cjs",
      "default": "./eslint.config.js"
    }
  },
  "files": [
    "eslint.config.js",
    "LICENCE",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config"
  ],
  "peerDependencies": {
    "eslint": ">= 8"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.5.0",
    "@types/confusing-browser-globals": "^1.0.3",
    "@types/eslint__js": "^8.42.3",
    "@types/eslint-plugin-markdown": "^2.0.2",
    "confusing-browser-globals": "^1.0.11",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-array-func": "^5.0.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^6.6.0",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-markdown": "^5.0.0",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-sonarjs": "^1.0.3",
    "eslint-plugin-tsdoc": "0.3.0",
    "eslint-plugin-unicorn": "^54.0.0",
    "eslint-plugin-write-good-comments": "^0.2.0",
    "globals": "^15.6.0",
    "typescript-eslint": "^8.0.0-alpha.10"
  },
  "devDependencies": {
    "@maxpatiiuk/prettier-config": "^2.0.0",
    "eslint": "^9.5.0",
    "prettier": "^3.3.2",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2"
  },
  "prettier": "@maxpatiiuk/prettier-config"
